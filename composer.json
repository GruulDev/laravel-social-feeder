{
    "name": "GruulDev/laravel-social-feeder",
    "description": "Social feeder for laravel. With this you can get feed from facebook page, twitter account or instragram",
    "keywords": ["framework", "laravel", "despark", "facebook", "instagram", "twitter", "feeder", "feed", "social-feeder"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yavor Mihaylov",
            "email": "ymihaylov@despark.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GruulDev/laravel-facebook-sdk",
            "no-api": true
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "GruulDev/laravel-facebook-sdk": "1.3-dev",
        "abraham/twitteroauth": "0.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "GruulDev\\LaravelSocialFeeder": "src/"
        }
    },
    "minimum-stability": "dev"
}
